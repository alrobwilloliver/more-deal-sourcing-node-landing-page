	<section class="features-section js--features-section" id="features">
		<div class="row">
			<h2>What we do</h2>
			<p>Hello, we are More Deal Sourcing. We search and find property investment opportunities for prospective
				investors.
				<!-- We also support deal sourcers to find properties for investors with our online search finding
				service. -->
			</p>
		</div>

		<div class="row">
			<div style="width: 70%; margin-left: 15%;">
				<div style=" margin-top: 15px;">
					<ion-icon class="icon-big" name="trending-up"></ion-icon>
					<h3>High Return On Investments</h3>
					<p class="long-copy">We provide reliable property investments with ROIs of at least
						<strong>20&percnt;</strong>.</p>
				</div>

				<div style="margin-top: 15px;">
					<ion-icon class="icon-big" name="search"></ion-icon>
					<h3>We find it so you don't have to</h3>
					<p class="long-copy">We understand that you want to save time finding high quality investments. We
						take out the headache and find it for you&excl;</p>
				</div>
				<div style=" margin-top: 15px;">
					<ion-icon class="icon-big" name="ios-text"></ion-icon>
					<h3>Provide Expert Advice</h3>
					<p class="long-copy">We stay up to date with rental and property market trends to provide quality
						advice for any investor.</p>
				</div>
			</div>
		</div>
	</section>

	<section class="investor-section js--investor-section" id="investor">
		<div class="row">
			<h2>Join Our Email List &ndash; Easy As 1, 2, 3</h2>
		</div>

		<div class="row">
			<div class="col span-1-of-3 box">
				<div>1</div>
				<h3 style="float: right; margin-top: 5px;">Input and send us your details below</h3>
			</div>
			<div class="col span-1-of-3 box">
				<div>2</div>
				<h3 style="float: right; margin-top: 5px;">Receive Weekly Property Deal Updates</h3>
			</div>
			<div class="col span-1-of-3 box">
				<div>3</div>
				<h3 style="float: right; margin-top: 5px;">Contact Us About The Deals You Like</h3>
			</div>
		</div>
		<form id="formAddUser" name="adduser" method="post">
			<% if(locals.red || locals.message === 'Something went wrong! Please try again.') { %>
			<div class="alert red" role="alert">
				<%=locals.message%>
			</div>
			<% } else { %>
			<div class="alert" role="alert">
				<%=locals.message%>
			</div>
			<% } %>
			<div class="row">
				<label for="inputUserName">Name:</label>
				<input type="text" id="inputUserName" name="username" required />
			</div>
			<div class="row" style="padding-left: 2px;">
				<label for="inputUserEmail">Email:</label>
				<input type="text" id="inputUserEmail" name="useremail" required />
			</div>
			<button style="margin-top: 20px; padding: 5px;" id="btnSubmit" type="submit">Join Email List</button>
		</form>
	</section>

