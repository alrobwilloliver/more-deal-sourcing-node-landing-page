					<ion-icon name="menu"></ion-icon>
				</a>
			</div>
		</nav>
		<div class="hero-text-box">
			<h1>We'll find the investment for you</h1>
			<a href="#" class="btn btn-full js--scroll-to-investor">I'm an investor</a>
			<!-- <a href="#" class="btn btn-ghost">I'm a deal sourcer</a> -->
